  "Template 1 for codeforces": {
    "prefix": "tem1",
    "body": [
      "/**",
      " *   author:  josuerom",
      " *   created: ${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR_SHORT} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}",
      "**/",
      "#include <bits/stdc++.h>",
      "using namespace std;\n",
      "#ifdef josuerom",
      "#include \"debug.h\"",
      "#else",
      "#define debug(...) 42",
      "#endif\n",
      "#define all(x)   (x).begin(),(x).end()",
      "#define rall(x)  (x).rbegin(),(x).rend()",
      "#define sort(x)  sort(all(x))",
      "#define rsort(x) sort(rall(x))",
      "#define ull      unsigned long long int",
      "#define ll       long long int",
      "#define mp       make_pair",
      "#define se       second",
      "#define fi       first\n",
      "$2\n",
      "void$3 solve() {",
      "}\n",
      "int main() {",
      "\tios::sync_with_stdio(false);",
      "\tcin.tie(nullptr); cout.tie(nullptr);",
      "\tint tt = 1;",
      "\tcin >> tt;$1",
      "\t#ifdef josuerom",
      "\t\tfor (int nc = 1; nc <= tt; nc++) {",
      "\t\t\tcout << \"----- Case #\" << nc << \" -----\\n\";",
      "\t\t\tsolve();",
      "\t\t}",
      "\t#else",
      "\t\twhile (tt--) solve();",
      "\t#endif",
      "\treturn 0;",
      "}"
    ],
    "description": "Plantilla óptima enfocada para codeforces"
  },
